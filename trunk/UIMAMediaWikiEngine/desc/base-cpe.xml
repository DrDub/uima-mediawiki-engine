<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <collectionReader>
        <collectionIterator>
            <descriptor>
                <import location="wikipedia-cr.xml"/>
            </descriptor>
            <configurationParameterSettings>
                <nameValuePair>
                    <name>InputXmlDump</name>
                    <value>
                        <string>/Users/Bowbaq/Downloads/frwikinews-20100420-pages-meta-history.xml</string>
                    </value>
                </nameValuePair>
                <nameValuePair>
                    <name>LatestRevisionOnly</name>
                    <value>
                        <boolean>true</boolean>
                    </value>
                </nameValuePair>
                <nameValuePair>
                    <name>IgnoreTalks</name>
                    <value>
                        <boolean>true</boolean>
                    </value>
                </nameValuePair>
            </configurationParameterSettings>
        </collectionIterator>
    </collectionReader>
    <casProcessors casPoolSize="3" processingUnitThreadCount="1">
        <casProcessor deployment="integrated" name="MWAnalysisEngine">
            <descriptor>
                <import location="wikipedia-ae.xml"/>
            </descriptor>
            <deploymentParameters/>
            <errorHandling>
                <errorRateThreshold action="terminate" value="0/1000"/>
                <maxConsecutiveRestarts action="terminate" value="30"/>
                <timeout max="100000" default="-1"/>
            </errorHandling>
            <checkpoint batch="10000" time="1000ms"/>
            <configurationParameterSettings>
                <nameValuePair>
                    <name>DefinitionFilePath</name>
                    <value>
                        <string>/Users/Bowbaq/Desktop/macros.txt</string>
                    </value>
                </nameValuePair>
            </configurationParameterSettings>
        </casProcessor>
        <casProcessor deployment="integrated" name="Xmi Writer CAS Consumer">
            <descriptor>
                <import location="../../../../../Downloads/apache-uima/examples/descriptors/cas_consumer/XmiWriterCasConsumer.xml"/>
            </descriptor>
            <deploymentParameters/>
            <errorHandling>
                <errorRateThreshold action="terminate" value="0/1000"/>
                <maxConsecutiveRestarts action="terminate" value="30"/>
                <timeout max="100000" default="-1"/>
            </errorHandling>
            <checkpoint batch="10000" time="1000ms"/>
            <configurationParameterSettings>
                <nameValuePair>
                    <name>OutputDirectory</name>
                    <value>
                        <string>/Users/Bowbaq/Desktop/test</string>
                    </value>
                </nameValuePair>
            </configurationParameterSettings>
        </casProcessor>
    </casProcessors>
    <cpeConfig>
        <numToProcess>-1</numToProcess>
        <deployAs>immediate</deployAs>
        <checkpoint batch="0" time="300000ms"/>
        <timerImpl/>
    </cpeConfig>
</cpeDescription>
